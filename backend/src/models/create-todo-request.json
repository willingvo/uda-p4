  "additionalProperties":false,
  "title":"todo",
  "type":"object",
  "properties":{
     "createdAt":{
        "type":"string"
     },
     "attachmentUrl":{
        "type":"string"
     },
     "dueDate":{
        "type":"string"
     },
     "name":{
        "type":"string",
